<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!-- This not a hard coded password... it's not even a password. And it's dynamically determined from system properties. -->
  <Match>
    <Or>
      <Class name="gov.va.api.health.vistafhirquery.tests.SystemDefinitions"/>
    </Or>
    <Bug pattern="HARD_CODE_PASSWORD"/>
  </Match>
</FindBugsFilter>
